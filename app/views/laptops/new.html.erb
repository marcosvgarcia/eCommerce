<%= form_for @laptop do |f| %>

  <p>
    <%= f.label :brand %><br>
    <%= f.text_field :brand %>
  </p>

  <p>
    <%= f.label :model %><br>
    <%= f.text_field :model %>
  </p>

  <p>
    <%= f.label :processorbrand %><br>
    <%= f.text_field :processorbrand %>
  </p>

  <p>
    <%= f.label :RAMsize %><br>
    <%= f.text_field :RAMsize %>
  </p>

  <p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<p>
  <%= link_to 'Back', laptops_path %>
</p>